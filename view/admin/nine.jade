			$("[name=all]").bind("click",function(){
				if ($(this).is(":checked")) {
					console.log("aa");
					$(".all32 input").each(function(){
						$(this).prop('checked', true);
					});
				}else {
					$(".all32 input").removeAttr("checked","checked");
				}
			});
			$(".all32 input").bind("click",function(){
				if ($(".all32 :checked").length < 32) {
					$("[name=all]").prop('checked', false);
				}else{
					$("[name=all]").prop('checked', true);
				}

			});
			$(".submit").click(function(){
				if ($("[name=cartype]").val() == "")
				{
					alert("请选择车型");
					return "";
				}
				$.ajax({
					url:$('[name=register]').attr("action"),
					type: 'POST',
					data: $('[name=register]').serializeArray(),
					dataType: 'json',
					context: $('body'), 
					success: function(msg){
						if (msg.recode == 200){
							window.location.href = "/admin/dealer/nine/"+msg.reason;
						}else{
							alert(msg.reason);
						}
					}
				});
			});
		});