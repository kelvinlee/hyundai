	script.
		$(document).ready(function(){
			$(".print-btn").click(function(){window.print()});
			$(".submit").click(function(){
				//- alert("sdf");
				$.ajax({
					url:$('[name=register]').attr("action"),
					type: 'POST',
					data: $('[name=register]').serializeArray(),
					dataType: 'json',
					context: $('body'), 
					success: function(msg){
						if (msg.recode == 200){
							window.location.reload();
						}else{
							alert(msg.reason);
						}
					}
				});
			});
		});